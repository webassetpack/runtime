{
  "name": "@wap/runtime",
  "version": "0.0.0-alpha.0",
  "description": "Web Asset Packs",
  "main": "lib/api.js",
  "types": "lib/api.d.ts",
  "scripts": {
    "build": "npm run lint && tsc && rollup -c rollup.config.js",
    "lint": "node ./node_modules/.bin/eslint --ext .ts '?(src|spec)/**/*.ts' --cache",
