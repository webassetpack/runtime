  },
  "keywords": [
    "web",
    "asset",
    "package",
    "pack"
  ],
  "homepage": "https://github.com/webassetpack/runtime",
  "repository": {
    "type": "git",
    "url": "https://github.com/webassetpack/runtime.git"
  },
  "author": "norman@normanbreau.com",
  "license": "Apache-2.0",
  "devDependencies": {
    "@breautek/jasmine": "3.5.1",
    "@rollup/plugin-commonjs": "11.1.0",
    "@rollup/plugin-node-resolve": "7.1.3",
    "@rollup/plugin-typescript": "4.1.1",
    "@totalpave/eslint-plugin": "3.0.1",
    "@types/jasmine": "3.5.10",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "eslint": "6.8.0",
    "jasmine-spec-reporter": "5.0.2",
    "nyc": "15.0.1",
    "rollup": "1.32.1",
    "rollup-plugin-progress": "1.1.1",
    "rollup-plugin-sourcemaps": "0.6.1",
    "rollup-plugin-typescript2": "0.27.0",
    "ts-node": "8.9.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@totalpave/interfaces": "1.0.0",
    "@types/node": "12.12.37",
    "@types/pako": "1.0.1",
    "@wap/core": "git+https://github.com/webassetpack/core.git",
    "pako": "1.0.11",
    "tslib": "1.11.2"
  }
}
